    <script>
        function dropdownclick() {
            document.getElementById("user_dropdown_1").style.visibility = "collapse";
        }
        var userclick = 0;

        function dropdownover_2() {
            document.getElementById("user_dropdown_1").style.opacity = 1;
            document.getElementById("user_dropdown_1").style.visibility = "visible";
        }

        function dropdownout_2() {
            document.getElementById("user_dropdown_1").style.opacity = 0;
            document.getElementById("user_dropdown_1").style.visibility = "collapse";
        }

        function dropdownclick_1() {
            if (userclick == 0) {
                document.getElementById("user_dropdown_1").style.opacity = 1;
                document.getElementById("user_dropdown_1").style.visibility = "visible";
                userclick++;
            } else {
                document.getElementById("user_dropdown_1").style.opacity = 0;
                document.getElementById("user_dropdown_1").style.visibility = "collapse";
                userclick--;
            }
        }


        window.onscroll = () => {
            if (document.querySelector('.header-block').getBoundingClientRect().top === 0) {
                document.querySelector('.header-block').style.boxShadow =
                    "rgb(187 187 187) 0 0 .5208333333333334vw 0"
            } else {
                document.querySelector('.header-block').style.boxShadow = ""
            }
        }
        document.body.addEventListener("click", function (evt) {
            console.dir(this);
            console.log(evt.target.tagName)
            if (evt.target.tagName != "I") {
                document.getElementById("user_dropdown_1").style.opacity = 0;
                document.getElementById("user_dropdown_1").style.visibility = "collapse";
                userclick = 0;
            }
        });
    </script>